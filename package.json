{
  "name": "quick-dine-service",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --target=bun --outdir=out",
    "start": "NODE_ENV=production bun src/index.ts",
    "test": "bun test",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "bun prisma studio",
    "prisma:reset": "bun run script:prisma:truncate && bun run prisma:seed",
    "prisma:setup": "bun run prisma:generate && bun run prisma:deploy",
    "script:prisma:truncate": "bun ./scripts/truncate-db.js",
    "script:generate:secret": "bun ./scripts/generate-secret.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/jwt": "^0.8.0",
    "@elysiajs/static": "^0.8.1",
    "@elysiajs/swagger": "^0.8.5",
    "@prisma/client": "^5.10.2",
    "@types/lodash": "^4.14.202",
    "elysia": "latest",
    "elysia-helmet": "^1.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prisma": "^5.10.2"
  },
  "module": "src/index.js"
}
