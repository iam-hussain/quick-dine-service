{
  "name": "quick-dine-service",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --target=bun --outdir=out",
    "start": "NODE_ENV=production bun src/index.ts",
    "test": "bun test",
    "prisma:deploy": "bunx --bun prisma migrate deploy",
    "prisma:generate": "bunx --bun prisma generate",
    "prisma:migrate": "bunx --bun prisma migrate dev",
    "prisma:pull": "bunx --bun prisma db pull",
    "prisma:push": "bunx --bun prisma db push",
    "prisma:seed": "bunx --bun prisma db seed",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npm run script:prisma:truncate && npm run prisma:seed",
    "prisma:setup": "npm run prisma:generate && npm run prisma:deploy",
    "script:prisma:truncate": "node ./scripts/truncate-db.mjs",
    "script:generate:secret": "node ./scripts/generate-secret.mjs"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/jwt": "^0.8.0",
    "@elysiajs/static": "^0.8.1",
    "@elysiajs/swagger": "^0.8.5",
    "@prisma/client": "^5.10.2",
    "@types/lodash": "^4.14.202",
    "elysia": "latest",
    "elysia-helmet": "^1.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prisma": "^5.10.2"
  },
  "module": "src/index.js"
}
